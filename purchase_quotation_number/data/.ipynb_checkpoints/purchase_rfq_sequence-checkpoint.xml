<?xml version="1.0" encoding="utf-8" ?>
<!-- Ajout d'une séquence PO dans le modèle ir.sequence -->
<odoo noupdate="0">
    <record id="purchase_po_sequence" model="ir.sequence">
        <field name="name">Purchase Request For Quotation</field>
        <field name="code">purchase.po</field>
        <field name="prefix">PO</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False" />
    </record>
</odoo>
