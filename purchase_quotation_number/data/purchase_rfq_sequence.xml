<?xml version="1.0" encoding="utf-8" ?>
<!-- Ajout d'une séquence dans le modèle ir.sequence -->
<odoo noupdate="0">
    <record id="purchase_rfq_sequence" model="ir.sequence">
        <field name="name">Purchase Request For Quotation</field>
        <field name="code">purchase.rfq</field>
        <field name="prefix">QO</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False" />
    </record>
</odoo>
