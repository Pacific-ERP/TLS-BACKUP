<?xml version="1.0" encoding="utf-8" ?>
<odoo><data>
    
<!-- Etiquette d'articles -->
    <!-- Action pour ouvrir le modèle des étiquettes d'articles -->
    <record model="ir.actions.act_window" id="product_etiquette_list_action">
            <field name="name">Étiquettes</field>
            <field name="res_model">product.etiquette</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Créer une étiquette
                </p>
            </field>
    </record>
    <!-- Ajout de notre menu au sous menu des configuration dans le modules inventaires -->
    <menuitem name="Étiquette d'articles" id="pe_revatua_etiquette" parent="stock.menu_product_in_config_stock" action="product_etiquette_list_action" sequence="10"/>
    
<!-- Communes -->
    <!-- Action pour ouvrir le modèle des communes -->
    <record model="ir.actions.act_window" id="contacts_commune_list_action">
            <field name="name">Communes</field>
            <field name="res_model">res.commune</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Créer une commune
                </p>
            </field>
    </record>
    <!-- Ajout de notre menu au sous menu des configuration dans le modules contacts -->
    <menuitem name="Communes" id="pe_revatua_communes" parent="contacts.menu_localisation" action="contacts_commune_list_action" sequence="2"/>
    
</data></odoo>
        